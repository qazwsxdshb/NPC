<?xml version="1.0" encoding="UTF-8"?>
<SceneNode>
    <node idText="Chapter0_start">
        <background pic="校園內工程"/>
        <dialogue name="【旁白】" pic="小N_發現了什麼">校園內的工程正如火如荼的進行中，某天雨後，小Ｎ和你來到校園放風，突然踩到了工人沒有收拾好的東西，摔了一跤之後跌入人孔蓋裡面</dialogue>
        <next>Chapter0_1</next>
    </node>
    <node idText="Chapter0_1">
        <dialogue name="【通知】" pic="一支火把">獲得一支火把</dialogue>
        <next>Chapter0_2</next>
    </node>
    <node idText="Chapter0_2">
        <background pic="水道"/>
        <dialogue name="【旁白】" pic="點亮的火把">小 N 點亮火把，沿著管道往下爬，遇上了兩個通往不同方向的水道</dialogue>
        <dialogue name="【旁白】" pic="點亮的火把">一個水道是清澈見底的，另一個水道上面佈滿了七彩的油墨</dialogue>
        <choice>
            <option next="Chapter0_pink_door">七彩油墨</option>
            <option next="Chapter0_black_door">清澈的水</option>
        </choice>
    </node>
    
    <node idText="Chapter0_black_door">
        <background pic="原始水道">Skip Animation</background>
        <dialogue name="【我】">哇嗚！水質那麼清澈，肯定會驚喜</dialogue>
        <next>Chapter1-2_start</next>
    </node>
    <node idText="Chapter0_pink_door">
        <background pic="顏料水道">Skip Animation</background>
        <dialogue name="【我】">七彩油墨，想想就覺得噁心</dialogue>
        <next>Chapter1-1_start</next>
    </node>


    <node idText="Chapter1-1_start">
        <background pic="黑色房間"/>
        <dialogue name="【旁白】">你選擇了布滿油汙的管道。突然入口噴出了又黏又臭的油汙，把你臭昏了</dialogue>
        <dialogue name="【旁白】">醒來之後發現你出現在一個黑暗無光的房間內。桌上有一個	正冒著煙</dialogue>
        <dialogue name="【旁白】">（a few minutes later）</dialogue>
        <dialogue name="【尼斯湖水怪】" pic="尼斯湖水怪1">你怎麼在這裡</dialogue>
        <dialogue name="【我】">我不小心走錯路了</dialogue>
        <dialogue name="【尼斯湖水怪】" pic="尼斯湖水怪1">（emmm…既然這樣的話）</dialogue>
        <dialogue name="【尼斯湖水怪】" pic="尼斯湖水怪1">我不要再為難你了！回答了問題就帶你去清洗一下身體吧～</dialogue>
        <dialogue name="【尼斯湖水怪】" pic="尼斯湖水怪1">請問今年第一個登陸的颱風是什麼名字?</dialogue>
        <choice>
            <option next="Chapter1-1_correct">凱米</option>
            <option next="Chapter1-1_wrong">杜蘇芮</option>
            <option next="Chapter1-1_wrong">海葵</option>
            <option next="Chapter1-1_wrong">蘇迪勒</option>
        </choice>
    </node>
    <node idText="Chapter1-1_correct">
        <dialogue name="【旁白】">正確！看來颱風假沒少放！</dialogue>
        <dialogue name="【通知】" pic="颱風">獲得暴風圈（有攻擊力）</dialogue>
        <dialogue name="【旁白】">於是你與小 N 帶著暴風圈繼續向下走。</dialogue>
        <next>Chapter1-1-1_start</next>
    </node>
    <node idText="Chapter1-1_wrong">
        <dialogue name="【旁白】">海葵為 2023 第一個登陸的颱風，杜蘇芮為 2023 年第一個颱風但未登陸，蘇迪勒颱風為 2015 年的颱風。</dialogue>
        <dialogue name="【通知】" pic="颱風眼">獲得颱風眼</dialogue>
        <dialogue name="【旁白】">於是你與小Ｎ帶著颱風眼繼續往下走</dialogue>
        <next>Chapter1-1-1_start</next>
    </node>

    <node idText="Chapter1-1-1_start">
        <background pic="淋浴間"/>
        <dialogue name="【旁白】" pic="小N_似乎在對話">對答完了後，水怪帶著我們來到淋浴間</dialogue>
        <dialogue name="【旁白】">看著正常的淋浴間，實則藏著深不可測的陷阱</dialogue>
        <dialogue name="" pic="蓮蓬頭">水龍頭噴出來的，不是水，而是42號混凝土，伴隨著一點一氧化碳</dialogue>
        <dialogue name="" pic="蛋">此時天上掉下一顆蛋</dialogue>
        <dialogue name="">小Ｎ實在是太餓太髒了，而且有點昏沉，已經分不清楚那是土還是混凝土</dialogue>
        <dialogue name="">於是...</dialogue>
        <dialogue name="【小 N】" pic="蛋">我們這樣拌在一起吃，是不是可以吃到美味的土雞蛋啊（下嚥）</dialogue>
        <dialogue name="【我】">啊啊啊啊啊不可以啊那是水泥！！</dialogue>
        <dialogue name="【旁白】">水泥中的矽酸鹽與雞蛋中的蛋白質產生化學反應，小Ｎ食用過多，目前已經一肚子火</dialogue>
        <dialogue name="" pic="石化小N">（小Ｎ已石化，化學反應導致熱脹冷縮，小Ｎ的外殼上布滿裂痕）</dialogue>
        <dialogue name="【尼斯湖水怪】" pic="尼斯湖水怪2">（竊笑）過我這關才沒有那麼簡單呢~~~~~~</dialogue>
        <dialogue name="【旁白】">如果你要拯救小Ｎ，你會選擇何種方式？</dialogue>
        <choice>
            <option next="Chapter1-1-1_A">哈姆立克法</option>
            <option next="Chapter1-1-1_B">灑水降溫法</option>
            <option next="Chapter1-1-1_C">激將法</option>
        </choice>
    </node>
    <node idText="Chapter1-1-1_A">
        <dialogue name="【旁白】" pic="石化小N">你使用了哈姆立克法，戳中了裂痕的交會處</dialogue>
        <dialogue name="" pic="石化小N">（轟）</dialogue>
        <dialogue name="" pic="冒火小N">（你把小Ｎ滿肚子的火發洩了出來。小Ｎ被解救了）</dialogue>
        <dialogue name="【旁白】" pic="尼斯湖水怪3">尼斯湖水怪被你的創舉燒死了！</dialogue>
        <dialogue name="【通知】" pic="尼斯湖水怪的精髓">獲得尼斯湖水怪的精髓</dialogue>
        <dialogue name="【旁白】">於是你與小Ｎ成功過了這關</dialogue>
        <next>Chapter2-1_start</next>
    </node>
    <node idText="Chapter1-1-1_B">
        <dialogue name="【旁白】" pic="茶壺2">你使用了灑水降溫法，把水壺裡面的水倒在了小Ｎ身上</dialogue>
        <dialogue name="【旁白】">水加劇了熱漲冷縮的效應</dialogue>
        <dialogue name="">（碎裂）</dialogue>
        <dialogue name="">（水把火滅了）</dialogue>
        <dialogue name="【旁白】" pic="小N_似乎在對話">水泥碎裂，一切都沒發生，小Ｎ變回了原狀</dialogue>
<!-- <ok> -->
        <dialogue name="【通知】" pic="">獲得水泥碎片</dialogue>
        <dialogue name="【尼斯湖水怪】" pic="尼斯湖水怪1">好啦看在你那麼機智的份上，我就暫時放了你們兩個吧！</dialogue>
        <next>Chapter2-2_start</next>
    </node>
    <node idText="Chapter1-1-1_C">
        <dialogue name="" pic="人形小N">（小Ｎ炸開，變為人型）</dialogue>
        <dialogue name="【小 N】" pic="人形小N">沒想到你居然敢跟我槓架！我都已經燒成這樣了你忍心嗎！！</dialogue>
        <dialogue name="" pic="颱風">（小Ｎ招來了凱米颱風，你被颱風捲走了）</dialogue>
        <next>GameoverScene</next>
    </node>

    <!-- bug -->
    <node idText="Chapter1-2_start">
        <background pic="粉紅房間"/>
        <dialogue name="【旁白】">你選擇了水質清澈的水道。水毫不留情地把你沖了下去。映入眼簾的是乾乾淨淨的房間</dialogue>
        <dialogue name="【旁白】">房間看起來毫無異常。突然從上面出現了一位美人魚公主</dialogue>
        <dialogue name="【美人魚公主】" pic="人魚公主1">你好，要不要和我一起互動呢？</dialogue>
        <dialogue name="【小 N】" pic="小N_似乎在對話">……（點頭）</dialogue>
        <dialogue name="【美人魚公主】" pic="人魚公主2">太好了！你說你是來探險的？（邪惡的笑容）</dialogue>
        <dialogue name="【美人魚公主】" pic="人魚公主1">看在你陪我聊天的份上，如果你能答對這道題目，我就讓一位得力助手跟著你吧！</dialogue>
        <dialogue name="【美人魚公主】" pic="人魚公主1">北科哪個地方沒有社辦（不含系學會）？</dialogue>
        <choice>
            <option next="Chapter1-2_wrong">中正館</option>
            <option next="Chapter1-2_wrong">綜合科館</option>
            <option next="Chapter1-2_wrong">宏裕科技研究大樓</option>
            <option next="Chapter1-2_correct">共同科館</option>
        </choice>
    </node>
    <node idText="Chapter1-2_correct">
        <dialogue name="【通知】" pic="大肌肌 Ken">獲得隊友—— 塑膠大肌肌</dialogue>
        <dialogue name="【旁白】">於是你與小 N 帶著塑膠製的猛男繼續向下走</dialogue>
        <next>Chapter2-3_start</next>
    </node>
    <node idText="Chapter1-2_wrong">
        <dialogue name="【小 N】" pic="小N_似乎在對話">……（搖頭）</dialogue>
        <next>Chapter1-2_start</next>
    </node>
    <!-- /bug -->

    <node idText="Chapter2-1_start">
        <background pic="池塘"/>
        <dialogue name="【旁白】">脫離了尼斯湖水怪之後，你轉身來到了一個池塘</dialogue>
        <dialogue name="【旁白】">池塘邊有個石頭，上面放著一本怪怪的書 你要閱讀嗎?</dialogue>
        <dialogue name="">要（接續下文）</dialogue>
        <background pic="颱風撞山"/>
        <dialogue name="【書】" pic="">凱米颱風生成才三天</dialogue>
        <dialogue name="【書】" pic="">就要往我們台灣筆直衝刺</dialogue>
        <dialogue name="【書】" pic="">就為了騎在台灣那粗大的中央山脈上</dialogue>
        <dialogue name="【書】" pic="">我已經預想到了</dialogue>
        <dialogue name="【凱米】">雜魚❤️雜魚❤️就憑這個雜魚山脈也想要阻止我嗎?</dialogue>
        <dialogue name="【中央山脈】">少瞧不起人了，我只是還沒有使出全力！</dialogue>
        <dialogue name="【凱米】">少嘴硬了❤️來吧來吧，快點ㄆㄧㄩ❤️ㄆㄧㄩ❤️的土石流吧❤️</dialogue>
        <dialogue name="【書】">此時經過了台灣最高峰——玉山</dialogue>
        <dialogue name="【書】">請問玉山多高?</dialogue>
        <choice>
            <option next="Chapter2-1_wrong">3952 km</option>
            <option next="Chapter2-1-1_start">3952 m</option>
            <option next="Chapter2-1_wrong">3925 m</option>
            <option next="Chapter2-1_wrong">3925 km</option>
        </choice>
    </node>
    <node idText="Chapter2-1_wrong">
        <dialogue name="【中央山脈】" pic="">居然不知道玉山多高？給我重選！</dialogue>
        <next>Chapter2-1_start</next>
    </node>
    <node idText="Chapter2-1-1_start">
        <background pic="颱風撞山"/>
        <dialogue name="【凱米】">啊❤️等⋯等等⋯⋯你怎麼突然變得這麼大！不行不行，要⋯要變弱颱了啊阿阿阿S</dialogue>
        <dialogue name="【中央山脈】">這才是我的全力！雖然台灣的土地很小，一直一直被別的國家嘲笑，但是！我的中央山脈可是很大的！！！給我變弱吧！！！</dialogue>
        <dialogue name="【凱米】">啊啊啊啊❤️我的颱風眼擴的好大❤️弱颱⋯弱颱⋯❤️要弱颱了啊啊啊啊啊❤️</dialogue>
        <dialogue name="">因為承受不住中央山脈的攻勢而變得衰弱的凱米那狂風暴雨也緩和了下來</dialogue>
        <dialogue name="">此時我正望著窗外的彩虹，將嘴裡的菸拿了下來，吐了一口氣</dialogue>
        <dialogue name="【我】">我到底寫了三小</dialogue>
        <dialogue name="">請問他到底寫了三小?</dialogue>
        <choice>
            <option next="Chapter2-1-1_correct">不知道</option>
        </choice>
    </node>
    <node idText="Chapter2-1-1_correct">
        <dialogue name="【通知】" pic="黑色筆記本2">獲得了—— 這本寶典秘笈</dialogue>
        <next>Chapter3-1_start</next>
    </node>




    <node idText="Chapter2-2_start">
        <background pic="池塘"/>
        <dialogue name="【旁白】">脫離了尼斯湖水怪之後，你轉身來到了一個池塘</dialogue>
        <dialogue name="【旁白】">池塘邊有個石頭，上面放著一本怪怪的書 你要閱讀嗎?</dialogue>
        <dialogue name="">要（接續下文）</dialogue>
        <background pic="颱風撞山"/>
        <dialogue name="【書】" pic="">凱米颱風生成才三天</dialogue>
        <dialogue name="【書】" pic="">就要往我們台灣筆直衝刺</dialogue>
        <dialogue name="【書】" pic="">就為了騎在台灣那粗大的中央山脈上</dialogue>
        <dialogue name="【書】" pic="">我已經預想到了</dialogue>
        <dialogue name="【凱米】">雜魚❤️雜魚❤️就憑這個雜魚山脈也想要阻止我嗎?</dialogue>
        <dialogue name="【中央山脈】">少瞧不起人了，我只是還沒有使出全力！</dialogue>
        <dialogue name="【凱米】">少嘴硬了❤️來吧來吧，快點ㄆㄧㄩ❤️ㄆㄧㄩ❤️的土石流吧❤️</dialogue>
        <dialogue name="">此時經過了台灣最高峰——玉山</dialogue>
        <dialogue name="">請問玉山多高?</dialogue>
        <choice>
            <option next="Chapter2-2_wrong">3952 km</option>
            <option next="Chapter2-2-1_start">3952 m</option>
            <option next="Chapter2-2_wrong">3925 m</option>
            <option next="Chapter2-2_wrong">3925 km</option>
        </choice>
    </node>
    <node idText="Chapter2-2_wrong">
        <dialogue name="【中央山脈】">居然不知道玉山多高？給我重選！</dialogue>
        <next>Chapter2-2_start</next>
    </node>
    <node idText="Chapter2-2-1_start">
        <background pic="颱風撞山"/>
        <dialogue name="【凱米】">啊❤️等⋯等等⋯⋯你怎麼突然變得這麼大！不行不行，要⋯要變弱颱了啊阿阿阿S</dialogue>
        <dialogue name="【中央山脈】">這才是我的全力！雖然台灣的土地很小，一直一直被別的國家嘲笑，但是！我的中央山脈可是很大的！！！給我變弱吧！！！</dialogue>
        <dialogue name="【凱米】">啊啊啊啊❤️我的颱風眼擴的好大❤️弱颱⋯弱颱⋯❤️要弱颱了啊啊啊啊啊❤️</dialogue>
        <dialogue name="">因為承受不住中央山脈的攻勢而變得衰弱的凱米那狂風暴雨也緩和了下來</dialogue>
        <dialogue name="">此時我正望著窗外的彩虹，將嘴裡的菸拿了下來，吐了一口氣</dialogue>
        <dialogue name="【我】">我到底寫了三小</dialogue>
        <dialogue name="">看完這本書之後，尼斯湖水怪突然又出現在面前</dialogue>
        <dialogue name="">你和小Ｎ被嚇到</dialogue>
        <dialogue name="【尼斯湖水怪】" pic="尼斯湖水怪2">嘿嘿嘿這裡就是我家，答對問題就給你這本書吧~</dialogue>
        <dialogue name="【尼斯湖水怪】" pic="尼斯湖水怪">下列吉祥物配對何者正確?</dialogue>
        <choice>
            <option next="Chapter2-2-1_wrong">Docker - 海星</option>
            <option next="Chapter2-2-1_wrong">Linux - 麻雀</option>
            <option next="Chapter2-2-1_correct">MySQL - 海豚</option>
            <option next="Chapter2-2-1_wrong">Rust - 龍蝦</option>
        </choice>
    </node>
    <node idText="Chapter2-2-1_correct">
        <dialogue name="【通知】" pic="黑色筆記本2">獲得了—— 這本寶典秘笈</dialogue>
        <next>Chapter3-2_start</next>
    </node>
    <node idText="Chapter2-2-1_wrong">
        <dialogue name="【通知】">你沒有獲得任何東西，但你還是需要繼續前進</dialogue>
        <next>Chapter3-2_start</next>
    </node>

    <node idText="Chapter2-3_start">
        <background pic="河床"/>
        <dialogue name="【旁白】">與美人魚公主道別後，被流水沖到了河床。適逢枯水期，到了中游沒水了</dialogue>
        <dialogue name="【旁白】" pic="紅包2">在中游時，一抹探頭探腦的身影引起了你的注意，你來不及多想，轉頭就看見 Ken 開始低頭，並從河床撿起一包紅包，來不及阻止，便見紅包內掉出一片榕樹葉</dialogue>
        <dialogue name="">（註：路上遇到紅包請不要亂撿）</dialogue>
        <dialogue name="【旁白】">四周頓時陰風陣陣，在你面前遠方突然一道閃電劃破了天際</dialogue>
        <dialogue name="【旁白】">你隱約聽到了哭聲</dialogue>
        <dialogue name="【某哭聲】" pic="皮諾丘1">我是冤死的皮諾丘，你們必須幫我申冤嗚嗚嗚……</dialogue>
        <dialogue name="" pic="皮諾丘1">（皮諾丘將臉龐遮住，使你們沒有看見他眼中異樣的情緒和微表情）</dialogue>
        <dialogue name="【皮諾丘】" pic="皮諾丘2">大家都知道，如果我說謊，我的鼻子就會變長，若否，則鼻子安然無恙。但如果我說：「我的鼻子要變長了！」，那麼，我的鼻子是會忽長忽短？變長？還是無事發生呢？他人擔心我說出這句話之後，鼻子忽長忽短，而我會調皮地拿這件事戲弄他人，就將我燒毀了，你們一定要找到事情真相？我是枉死的嗎？</dialogue>
        <choice>
            <option next="Chapter2-3_correct">是，你是枉死的，你的鼻子根本不會變長！</option>
            <option next="Chapter2-3_wrong">是，你是枉死的，你的鼻子會變長！</option>
            <option next="Chapter2-3_wrong">否，你的鼻子確實會忽長忽短！</option>
        </choice>
    </node>
    <node idText="Chapter2-3_correct">
        <dialogue name="">大肌肌 Ken 答對了問題，皮諾丘非常開心</dialogue>
